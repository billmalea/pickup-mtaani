{
  "name": "pickup-mtaani-sdk",
  "version": "1.0.0",
  "description": "Official Node.js/TypeScript SDK for Pickup Mtaani API - Kenya's leading delivery platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "pickup-mtaani",
    "delivery",
    "kenya",
    "api",
    "sdk",
    "typescript",
    "mpesa",
    "logistics",
    "courier",
    "shipping"
  ],
  "author": "Pickup Mtaani SDK Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/billmalea/pickup-mtaani.git"
  },
  "bugs": {
    "url": "https://github.com/billmalea/pickup-mtaani/issues"
  },
  "homepage": "https://github.com/billmalea/pickup-mtaani#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
